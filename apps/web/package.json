{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "deploy": "bun run build && wrangler deploy",
    "format": "bunx biome format --write .",
    "lint": "bunx biome lint --write .",
    "check": "bunx biome check --write .",
    "cf-types": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "d1:prod:local": "bun run wrangler d1 migrations apply funds-db-prod --local",
    "d1:prod:remote": "bun run wrangler d1 migrations apply funds-db-prod --remote",
    "d1:dev:remote": "bun run wrangler d1 migrations apply budget-planner-dev --remote",
    "d1:dev:local": "bun run wrangler d1 migrations apply budget-planner-dev --local"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.120.15",
    "@types/bun": "^1.2.5",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.5.1",
    "@cloudflare/workers-types": "^4.20250604.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^6.2.5",
    "wrangler": "^4.19.1",
    "vite-plugin-pwa": "^0.21.2"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.2.3",
    "@hono/arktype-validator": "^2.0.1",
    "@hookform/resolvers": "^5.0.1",
    "@intentui/icons": "^1.10.33",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-query": "^5.80.5",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@tanstack/react-router": "^1.120.15",
    "@tanstack/router-devtools": "^1.120.15",
    "arktype": "^2.1.20",
    "better-auth": "^1.2.7",
    "drizzle-arktype": "^0.1.3",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.7.11",
    "motion": "^12.7.4",
    "react": "^19.0.0",
    "react-aria-components": "^1.7.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "sonner": "^2.0.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.2.1"
}
